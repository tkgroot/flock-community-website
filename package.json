{
  "name": "flock-community-website",
  "version": "1.0.0",
  "description": "The flock community website",
  "homepage": "https://www.flock.community",
  "main": "./src/index.html",
  "author": "Willem Kersten",
  "license": "ISC",
  "scripts": {
    "gatsby": "gatsby",
    "build": "gatsby build",
    "develop": "gatsby develop --host 0.0.0.0 --port 8000",
    "develop:sync": "browser-sync",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "concurrently \"npm:storybook\" \"npm:develop\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "storybook": "start-storybook --host 0.0.0.0 --port 9001",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-storybook": "build-storybook",
    "deploy": "gcloud app deploy --project flock-community-website"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "rfs": "^9.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/format": "^11.0.0",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-essentials": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/react": "^6.1.9",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.13",
    "concurrently": "^5.3.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^2.27.4",
    "gatsby-cli": "^2.14.1",
    "gatsby-image": "^2.6.0",
    "gatsby-plugin-manifest": "^2.7.0",
    "gatsby-plugin-offline": "^3.5.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-sass": "^2.6.0",
    "gatsby-plugin-sharp": "^2.9.1",
    "gatsby-source-filesystem": "^2.6.1",
    "gatsby-transformer-remark": "^2.11.1",
    "gatsby-transformer-sharp": "^2.7.0",
    "lerna": "^3.22.1",
    "node-sass": "^4.14.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2"
  },
  "browserslist": [
    "defaults",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flock-community/flock-community-website"
  },
  "bugs": {
    "url": "https://github.com/flock-community/flock-community-website/issues"
  }
